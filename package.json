{
	"name": "array-reduce-group-by",
	"version": "1.0.0",
	"description": "A group-by function working with the built-in Array.reduce()",
	"license": "MIT",
	"repository": "EdJoPaTo/array-reduce-group-by",
	"author": {
		"name": "EdJoPaTo",
		"email": "array-reduce-group-by-npm-package@edjopato.de",
		"url": "https://edjopato.de"
	},
	"scripts": {
		"build": "del-cli dist && tsc",
		"prepack": "npm run build",
		"test": "tsc --sourceMap && xo && nyc ava"
	},
	"type": "commonjs",
	"engines": {
		"node": ">=12"
	},
	"devDependencies": {
		"@sindresorhus/tsconfig": "^1.0.2",
		"@types/node": "^16.0.0",
		"ava": "^3.14.0",
		"del-cli": "^4.0.0",
		"nyc": "^15.0.0",
		"typescript": "^4.2.3",
		"xo": "^0.42.0"
	},
	"files": [
		"dist/source",
		"!*.test.*"
	],
	"main": "dist/source",
	"types": "dist/source",
	"nyc": {
		"all": true,
		"reporter": [
			"lcov",
			"text"
		]
	},
	"xo": {
		"rules": {
			"@typescript-eslint/prefer-readonly-parameter-types": "warn",
			"ava/no-ignored-test-files": "off"
		},
		"overrides": [
			{
				"files": [
					"**/*.test.*",
					"test/**/*.*"
				],
				"rules": {
					"@typescript-eslint/prefer-readonly-parameter-types": "off"
				}
			}
		]
	}
}
